p down-->\r\n    <div class=\"Destino\">\r\n      \r\n      <label for=\"cars\">Destino:</label>\r\n\r\n      <select name=\"cars\" id=\"cars\">\r\n        <option value=\"volvo\">Volvo</option>\r\n        <option value=\"saab\">Saab</option>\r\n        <option value=\"mercedes\">Mercedes</option>\r\n        <option value=\"audi\">Audi</option>\r\n      </select>\r\n    </div>\r\n\r\n    <div class=\"Peso\">\r\n      <big>Peso</big>\r\n      <div>\r\n        <input type=\"number\" placeholder=\"Insira aqui o peso da carga em Kg\" style=\"width: 300px; font-size:large\" >\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"Botao\">\r\n      <button style=\" background-color: rgb(147, 196, 125) !important;\"> Analisar </button>\r\n    </div>\r\n\r\n    <b >{{\"Estas são as melhores alternativas de frete que encontramos pra você.\"}}</b>\r\n    <hr style=\"width: 350px\" size=\"4px\"> \r\n    \r\n\r\n    <div class=\"result\">\r\n      <p>Frete mais barato:{{cheaper}}</p>\r\n      <p>Frete mais rapido:{{fastest}}</p>\r\n\r\n    </div>\r\n    \r\n  </main>\r\n\r\n \r\n  </div>\r\n\r\n\r\n</template>\r\n\r\n<script>\r\n\r\n\r\nimport {\r\n  BNavbar,\r\n  BNavbarBrand, \r\n} from 'bootstrap-vue'\r\nimport { FSWatcher } from 'chokidar';\r\n\r\nexport default {\r\n  components: {\r\n    BNavbar,\r\n    BNavbarBrand,\r\n  },\r\n  data() {\r\n    const appName = ''\r\n    const cheaper = ''\r\n    const fastest = ''\r\n\r\n    return {\r\n      appName,\r\n      cheaper,\r\n      fastest,\r\n    }\r\n  },\r\n  created() {\r\n    // Implemente aqui o GET dos dados da API REST\r\n    // para que isso ocorra na inicialização da pagina\r\n    this.appName = 'MELHOR FRETE'\r\n    this.cheaper = \"ain\"\r\n    this.fastest = \"euin\"\r\n  },\r\n  methods: {\r\n    // Implemente aqui os metodos utilizados na pagina\r\n    methodFoo() {\r\n      console.log(this.appName)\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.title .navbar {\r\n  background-color: rgb(147, 19